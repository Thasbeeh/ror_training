<h1>Log in</h1>


 <!-- data: { turbo: false } -->
<%= form_with(scope: :session, url: login_path, data: { turbo: false }) do |form| %>
  
  <div>
    <%= form.label :email, style: "display: block" %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password, style: "display: block" %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit "Log in"%>
  </div>
<% end %>

